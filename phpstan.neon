parameters:
    level: max
    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: true
    symfony:
        containerXmlPath: var/cache/dev/App_KernelDevDebugContainer.xml
    paths:
        - src
    ignoreErrors:
        - '#.::\$id is never written, only read.#'
