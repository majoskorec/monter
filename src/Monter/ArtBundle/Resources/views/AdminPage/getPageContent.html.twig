{% for typ, data in page.pageImages %}
<div class="">
    <div class="text-bold">
        {% set name = 'image.type.' ~ typ %}
        {{ name|trans({},'messages') }}
    </div>
    {% if data.isSet %}
    <div class="">
        <img src="data:image/png;base64, {{ data.data }}" alt="{{ data.title }}" />
    </div>
    {% endif %}
</div>
{% endfor %}