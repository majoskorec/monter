{% extends "::base.html.twig" %}

{% block title %}Monter{% endblock title %}

{% block stylesheets %}
<link rel="stylesheet" href="{{ asset('bundles/monterart/css/bootstrap.min.css') }}">
<link rel="stylesheet" href="{{ asset('bundles/monterart/css/bootstrap-theme.min.css') }}">
<link rel="stylesheet" href="{{ asset('bundles/monterart/css/main.css') }}">
{% endblock %}

{% block javascripts %}
<script type="text/javascript" src="{{ asset('bundles/monterart/js/jquery.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/monterart/js/bootstrap.min.js') }}"></script>
<script type="text/javascript">

    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-1448359-10']);
    _gaq.push(['_setDomainName', 'monter.sk']);
    _gaq.push(['_trackPageview']);

    (function() {
        var ga = document.createElement('script');
        ga.type = 'text/javascript';
        ga.async = true;
        ga.src = ('https:' === document.location.protocol ? 'https://' : 'http://') + 'stats.g.doubleclick.net/dc.js';
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(ga, s);
    })();

</script>
{% endblock javascripts %}

{% block body %}
<div class="container">
    <div class="header">
    {% block pageHeader %}
    {% endblock pageHeader %}
    </div>
    <div class="content">
    {% block content %}

    {% endblock content %}
    </div>
    {% block backButton %}
    {% endblock backButton %}
</div>
{% endblock body %}